{
  "env": {
    "myDefaultIncludePath": ["${workspaceFolder}", "${workspaceFolder}"],
    "myCompilerPath": "/Library/Developer/CommandLineTools/usr/bin/gcc"
  },
  "configurations": [
    {
      "name": "GCC C89",
      "intelliSenseMode": "clang-x64",
      "includePath": ["${myDefaultIncludePath}"],
      "macFrameworkPath": ["/System/Library/Frameworks"],
      "defines": ["FOO", "BAR=100"],
      "compilerPath": "/Library/Developer/CommandLineTools/usr/bin/gcc",
      "cStandard": "c89",
      "cppStandard": "c++17",
      "browse": {
        "path": ["${workspaceFolder}"],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
  ],
  "version": 4
}
